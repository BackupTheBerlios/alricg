		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="kommentar" type="xs:string" minOccurs="0"/>
				<xs:element name="datenBasis" type="dataBaseTyp" minOccurs="0"/>
				<xs:element name="version" type="xs:string" minOccurs="0"/>
				<xs:element name="rassen" type="rasseTyp" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="kulturen" type="kulturTyp" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element name="professionen" type="professionTyp" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="dataBaseTyp">
		<xs:sequence>
			<xs:element name="zauberMerkmal">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="merkmal" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="bez" type="xs:string" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="liturgieHerkunft">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="herkunft" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="bez" type="xs:string" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="ritualHerkunft">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="herkunft" maxOccurs="unbounded">
							<xs:complexType>
								<xs:attribute name="bez" type="xs:string" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ############################################ RASSE ########################################### -->
	<xs:complexType name="rasseTyp">
		<xs:sequence>
			<xs:element name="gp" type="xs:nonNegativeInteger"/>
			<xs:element name="varianteVon" type="xs:string" minOccurs="0"/>
			<xs:element name="lepModi" type="xs:int"/>
			<xs:element name="aupModi" type="xs:int"/>
			<xs:element name="aspModi" type="xs:int"/>
			<xs:element name="kaModi" type="xs:int"/>
			<xs:element name="mrModi" type="xs:int"/>
			<xs:element name="eigenschaftModi" type="eigenschaftModiType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="vorteile" type="vorteileHerkunftType" minOccurs="0"/>
			<xs:element name="nachteile" type="nachteileHerkunftType" minOccurs="0"/>
			<xs:element name="sonderf" type="sonderfHerkunftType" minOccurs="0"/>
			<xs:element name="groesse">
				<xs:complexType>
					<xs:attribute name="groesse" type="xs:positiveInteger" use="required"/>
					<xs:attribute name="anzWuerfel" type="xs:positiveInteger" use="required"/>
					<xs:attribute name="augenWuerfel" type="xs:positiveInteger" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="gewicht">
				<xs:complexType>
					<xs:attribute name="gewicht" type="xs:negativeInteger" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="haarfarbe">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="farbe" maxOccurs="20">
							<xs:complexType>
								<xs:attribute name="farbe" type="xs:string" use="required"/>
								<xs:attribute name="wuerfelWurf" type="xs:positiveInteger" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="augenfarbe">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="farbe" maxOccurs="20">
							<xs:complexType>
								<xs:attribute name="farbe" type="xs:string" use="required"/>
								<xs:attribute name="wuerfelWurf" type="xs:positiveInteger" use="required"/>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="alter">
				<xs:complexType>
					<xs:attribute name="von" type="xs:positiveInteger" use="required"/>
					<xs:attribute name="bis" type="xs:positiveInteger" use="required"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="talente" type="talenteHerkunftType" minOccurs="0"/>
			<xs:element name="beschreibung" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="id" type="xs:string" use="required"/>
	</xs:complexType>
	<!-- ############################################ KULTUR ########################################### -->
	<xs:complexType name="kulturTyp">
		<xs:sequence>
			<xs:element name="gp" type="xs:nonNegativeInteger"/>
			<xs:element name="varianteVon" type="xs:string" minOccurs="0"/>
