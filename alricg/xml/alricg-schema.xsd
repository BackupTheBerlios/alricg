			<xs:element name="beschreibung" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="id" type="xs:string" use="required"/>
	</xs:complexType>
	<!-- ############################################ PROFESSION ########################################### -->
	<xs:complexType name="professionTyp">
		<xs:sequence>
			<xs:element name="gp" type="xs:nonNegativeInteger"/>
			<xs:element name="varianteVon" type="xs:string" minOccurs="0"/>
			<xs:element name="zeitaufwendig" type="xs:boolean" minOccurs="0"/>
			<xs:element name="erstprofession" type="xs:boolean" minOccurs="0"/>
			<xs:element name="voraussetzungen" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="eigenschaft" type="eigenschaftModiType" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
				<!-- Es muÃŸ nur EINE der Voraussetzungen gelten! (So werden die "oders" nachgebildet) -->
			</xs:element>
			<xs:element name="art">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="handwerklich"/>
