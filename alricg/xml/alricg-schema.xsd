					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="lepModi" type="xs:int"/>
			<xs:element name="aupModi" type="xs:int"/>
			<xs:element name="aspModi" type="xs:int"/>
			<xs:element name="kaModi" type="xs:int"/>
			<xs:element name="mrModi" type="xs:int"/>
			<xs:element name="soModi" type="xs:int"/>
			<xs:element name="soMax" type="xs:int"/>
			<xs:element name="soMin" type="xs:int"/>
			<xs:element name="eigenschaftModi" type="eigenschaftModiType" minOccurs="0" maxOccurs="unbounded"/>
			<xs:element name="vorteile" type="vorteileHerkunftType" minOccurs="0"/>
			<xs:element name="nachteile" type="nachteileHerkunftType" minOccurs="0"/>
			<xs:element name="sonderf" type="sonderfHerkunftType" minOccurs="0"/>
			<xs:element name="kulturenUeblich" type="idListe" minOccurs="0"/>
			<xs:element name="kulturenMoeglich" type="idListe" minOccurs="0"/>
			<xs:element name="talente" type="talenteHerkunftType" minOccurs="0"/>
			<xs:element name="spracheUndSchrift" type="spracheSchriftHerkunftType" minOccurs="0"/>
			<xs:element name="liturgien" type="liturgieRitualHerkunftType" minOccurs="0"/>
			<xs:element name="rituale" type="liturgieRitualHerkunftType" minOccurs="0"/>
