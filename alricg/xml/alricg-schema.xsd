			<xs:element name="besondererBesitz" type="xs:string"/>
			<xs:element name="beschreibung" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="id" type="xs:string" use="required"/>
	</xs:complexType>
	<!-- ############################################ Sonderfertigkeit ########################################### -->
	<xs:complexType name="sondefType">
		<xs:sequence>
			<xs:element name="gp" type="xs:nonNegativeInteger"/>
			<xs:element name="ap" type="xs:nonNegativeInteger"/>
			<xs:element name="istWaehlbar" type="booleanType"/>
			<xs:element name="art">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="waffenlosKampf"/>
						<xs:enumeration value="bewaffentKampf"/>
						<xs:enumeration value="magisch"/>
						<xs:enumeration value="geweiht"/>
						<xs:enumeration value="schamanisch"/>
						<xs:enumeration value="sonstige"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="fuerWelcheChars" type="charArtenType" maxOccurs="6"/>
			<xs:element name="voraussetzungen" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="sonderf" type="xs:NMTOKEN" minOccurs="0"/>
						<xs:element name="eigenschaft" type="eigenschaftModiType" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="talent" type="idUndWert" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="zauber" type="idUndWert" minOccurs="0" maxOccurs="unbounded"/>
						<xs:element name="PA" type="xs:positiveInteger" minOccurs="0"/>
						<xs:element name="AT" type="xs:positiveInteger" minOccurs="0"/>
						<xs:element name="INI" type="xs:positiveInteger" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
				<!-- Es muÃŸ nur EINE der Voraussetzungen gelten! (So werden die "oders" nachgebildet) -->
			</xs:element>
			<xs:element name="beschreibung" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="id" type="xs:string" use="required"/>
	</xs:complexType>
	<!-- ############################################ Vorteil ########################################### -->
	<xs:complexType name="vorteilType">
		<xs:sequence>
			<xs:element name="gp" type="xs:nonNegativeInteger"/>
			<xs:element name="istWaehlbar" type="xs:boolean"/>
			<xs:element name="istGabe" type="xs:boolean"/>
			<xs:element name="istMehrfachWaehlbar" type="xs:boolean"/>
			<xs:element name="minStufe" type="xs:positiveInteger"/>
