		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="id" type="xs:string" use="required"/>
	</xs:complexType>
	<!-- ############################################ Sprache ########################################### -->
	<xs:complexType name="sprachType">
		<xs:sequence>
			<xs:element name="kostenKlasse" type="kostenKlasseType"/>
			<xs:element name="komplexitaet" type="xs:positiveInteger"/>
			<xs:element name="beschreibung" type="xs:string" minOccurs="0"/>
		</xs:sequence>
