		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="id" type="xs:string" use="required"/>
	</xs:complexType>
	<!-- ############################################ Zauber ########################################### -->
	<xs:complexType name="zauberType">
		<xs:sequence>
			<xs:element name="kostenKlasse" type="kostenKlasseType"/>
			<xs:element name="merkmale" type="magieMerkmaleType" maxOccurs="unbounded"/>
