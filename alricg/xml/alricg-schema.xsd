			<xs:element name="spezialRegeln" type="xs:int" minOccurs="0"/>
			<xs:element name="fuerWelcheChars" type="charArtenType" maxOccurs="5"/>
			<xs:element name="verbietetVorteil" type="idListe" minOccurs="0"/>
			<xs:element name="verbietetNachteil" type="idListe" minOccurs="0"/>
