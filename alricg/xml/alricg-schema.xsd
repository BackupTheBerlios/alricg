	<!-- ############################################ Nachteil ########################################### -->
	<xs:complexType name="nachteilType">
		<xs:sequence>
			<xs:element name="gp" type="xs:nonPositiveInteger"/>
			<xs:element name="istWaehlbar" type="xs:boolean"/>
			<xs:element name="istSchlechteEigen" type="xs:boolean"/>
			<xs:element name="istMehrfachWaehlbar" type="xs:boolean"/>
			<xs:element name="minStufe" type="xs:positiveInteger"/>
			<xs:element name="maxStufe" type="xs:positiveInteger"/>
			<xs:element name="spezialRegeln" type="xs:int" minOccurs="0"/>
			<xs:element name="fuerWelcheChars" type="charArtenType" maxOccurs="5"/>
			<xs:element name="verbietetVorteil" type="idListe" minOccurs="0"/>
			<xs:element name="verbietetNachteil" type="idListe" minOccurs="0"/>
			<xs:element name="beschreibung" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
