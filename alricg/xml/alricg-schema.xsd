	</xs:complexType>
	<!-- ############################################ Talent ########################################### -->
	<xs:complexType name="talentType">
		<xs:sequence>
			<xs:element name="art">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="basis"/>
						<xs:enumeration value="spezial"/>
						<xs:enumeration value="beruf"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="sorte">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="kampf"/>
						<xs:enumeration value="koerper"/>
						<xs:enumeration value="gesellschaft"/>
						<xs:enumeration value="natur"/>
