<xsd:schema targetNamespace="http://www.die3sphaere.de/alricg/held/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.die3sphaere.de/alricg/held/">
	<xsd:annotation>
		<xsd:documentation xml:lang="DE">

		</xsd:documentation>
	</xsd:annotation>
	<xsd:element name="alricgXML">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="allgemeineDaten" type="allgemeineDatenTyp"/>
				<xsd:element name="basisWerte" type="basisWerteTyp"/>
				<xsd:element name="herkunft" type="herkunftTyp"/>
				<xsd:element name="vorteile" type="vorteilTyp"/>
				<xsd:element name="nachteile" type="nachteilTyp"/>
				<xsd:element name="sonderfertigkeiten" type="sonderfertigkeitTyp"/>
				<xsd:element name="sprachen" type="sprachTyp"/>
				<xsd:element name="schriften" type="schriftTyp"/>
				<xsd:element name="talente" type="talenteTyp"/>
				<xsd:element name="zauber" type="zauberTyp" minOccurs="0"/>
				<xsd:element name="liturgien" type="ritualLiturgieTyp" minOccurs="0"/>
				<xsd:element name="rituale" type="ritualLiturgieTyp" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<!-- *************************** Allgemeine Daten ***************************** -->
	<xsd:complexType name="allgemeineDatenTyp">
		<xsd:sequence>
			<xsd:element name="name" type="xsd:string"/>
			<xsd:element name="titel" type="xsd:string" minOccurs="0"/>
			<xsd:element name="stand" type="xsd:string" minOccurs="0"/>
			<xsd:element name="geschlecht" type="xsd:string"/>
			<xsd:element name="alter" type="xsd:positiveInteger"/>
			<xsd:element name="groesse" type="xsd:positiveInteger"/>
			<xsd:element name="haarfarbe" type="xsd:string"/>
			<xsd:element name="augenfarbe" type="xsd:string"/>
			<xsd:element name="sozialstatus" type="xsd:string"/>
			<xsd:element name="aussehen" type="xsd:string" minOccurs="0"/>
			<xsd:element name="herkunft" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ***************************** Herkunft ******************************* -->
	<xsd:complexType name="herkunftTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
					zweitKultur - F�nd zweier Welten" siehe S.1 124
					zweitProfession - F�eitgef㢨erte Bildung" S. 124
				</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="rasse" type="xsd:string"/>
			<xsd:element name="kultur" type="xsd:string"/>
			<xsd:element name="zweitKultur" type="xsd:string"/>
			<xsd:element name="profession" type="profHilfsTyp"/>
			<xsd:element name="zweitProfession" type="profHilfsTyp"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="profHilfsTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
					abgebrochen - Diese Profession ist Abgebrochen (siehe AH S.124)
					veteran - Diese Profession mit dem Vorteil "Veteran" verbunden
				</xsd:documentation>
		</xsd:annotation>
		<xsd:simpleContent>
			<xsd:extension base="xsd:string">
				<xsd:attribute name="abgebrochen" type="xsd:boolean" use="optional"/>
				<xsd:attribute name="veteran" type="xsd:boolean" use="optional"/>
			</xsd:extension>
		</xsd:simpleContent>
	</xsd:complexType>
	<!-- ***************************** Basis Werte ******************************* -->
	<xsd:complexType name="basisWerteTyp">
		<xsd:sequence>
			<xsd:element name="eigenschaften">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="MU" type="heldElementTyp"/>
						<xsd:element name="KL" type="heldElementTyp"/>
						<xsd:element name="IN" type="heldElementTyp"/>
						<xsd:element name="CH" type="heldElementTyp"/>
						<xsd:element name="FF" type="heldElementTyp"/>
						<xsd:element name="GE" type="heldElementTyp"/>
						<xsd:element name="KO" type="heldElementTyp"/>
						<xsd:element name="KK" type="heldElementTyp"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="erweiterteEigenschaften">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="LeP" type="heldElementTyp"/>
						<xsd:element name="AsP" type="heldElementTyp"/>
						<xsd:element name="AuP" type="heldElementTyp"/>
						<xsd:element name="KA" type="heldElementTyp"/>
						<xsd:element name="MR" type="heldElementTyp"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="abenteuerPunkte">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="apInsgesamt" type="xsd:int"/>
						<xsd:element name="apGuthaben" type="xsd:int"/>
						<xsd:element name="apLog" type="logTyp"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ***************************** Talente ******************************* -->
	<xsd:complexType name="talenteTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
				leitwert - F�hteil "Elfische Weltsicht"
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="talent" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="heldElementTyp">
							<xsd:attribute name="leitwert" type="xsd:boolean" use="optional"/>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ***************************** Zauber ******************************* -->
	<xsd:complexType name="zauberTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
				repr㲥ntation - die Repr㲥ntation in der der Zauber gelernt wurde
				hauszauber - Ist der Zauber ein Hauszauber?
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="zauber" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="heldElementTyp">
							<xsd:attribute name="repr㲥ntation" type="xsd:string" use="required"/>
							<xsd:attribute name="hauszauber" type="xsd:boolean" use="required"/>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ***************************** Sonderf ******************************* -->
	<xsd:complexType name="sonderfertigkeitTyp">
		<xsd:sequence>
			<xsd:element name="vorteil" type="heldElementTyp" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ***************************** Vorteile ******************************* -->
	<xsd:complexType name="vorteilTyp">
		<xsd:sequence>
			<xsd:element name="vorteil" type="heldElementTyp" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ***************************** Nachteile ******************************* -->
	<xsd:complexType name="nachteilTyp">
		<xsd:sequence>
			<xsd:element name="nachteil" type="heldElementTyp" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ************************* Rituale & Liturgien ************************* -->
	<xsd:complexType name="ritualLiturgieTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
				herkunft - in welcher Herkunft (Repr㲥ntation) der Ritus gelernt wurde
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ritus" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:complexContent>
						<xsd:extension base="heldElementTyp">
							<xsd:sequence>
								<xsd:element name="herkunft" type="xsd:string"/>
							</xsd:sequence>
						</xsd:extension>
					</xsd:complexContent>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ******************************** Sprache ******************************* -->
	<xsd:complexType name="sprachTyp">
		<xsd:sequence>
			<xsd:element name="muttersprache" type="heldElementTyp"/>
			<xsd:element name="zweitsprache" type="heldElementTyp" minOccurs="0"/>
			<xsd:element name="weitereSprache" type="heldElementTyp" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ******************************** Schrift ******************************* -->
	<xsd:complexType name="schriftTyp">
		<xsd:sequence>
			<xsd:element name="schrift" type="heldElementTyp" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ******************************** Ausr� ******************************** -->
	<xsd:complexType name="ausruestungTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
				einordnung - Zur besseren Sortierung. Hier kann etwas wie "Pflanze", "Trank", "Kleidung" o.⬠angegeben werden.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="gegenstand" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="gewicht" type="xsd:nonNegativeInteger" minOccurs="0"/>
						<xsd:element name="wert" type="xsd:nonNegativeInteger" minOccurs="0"/>
						<xsd:element name="anzahl" type="xsd:int"/>
						<xsd:element name="haltbarkeit" type="xsd:string"/>
						<xsd:element name="woGetragen" type="xsd:string" minOccurs="0"/>
						<xsd:element name="anmerkung" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
					<xsd:attribute name="bezeichnung" type="xsd:string" use="required"/>
					<xsd:attribute name="einordnung" type="xsd:string" use="optional"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ********************************  Verm� ******************************** -->
	<xsd:complexType name="geldTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
				log - Die Entwicklung des Verm�s in Kreuzer
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="vermoegen" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="aufbewarungsOrt" type="xsd:string"/>
						<xsd:element name="dukaten" type="xsd:int"/>
						<xsd:element name="silber" type="xsd:int"/>
						<xsd:element name="heller" type="xsd:int"/>
						<xsd:element name="kreuzer" type="xsd:int"/>
						<xsd:element name="log" type="logTyp"/>
						<xsd:element name="anmerkung" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- **********************************  Waffen  ********************************* -->
	<xsd:complexType name="waffenTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
				tp-W6 - Anzahl an W6
				tp-W20 - Anzahl an W20
				tp-plus - Hinzuaddierte Trefferpunkte
				tp-kk-ab - Ab wann es einen +1 Trefferpunkte-Bonus gibt
				tp-kk-stufe - Ab wann es weiter (ja +1) TP-Boni gibt
				wm-at - WM auf AT
				wm-pa - WM auf PA
				talentId - Mit welchen Talent(en) diese Waffe angewendet werden kann.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="waffe" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="tp-W6" type="xsd:nonNegativeInteger"/>
						<xsd:element name="tp-W20" type="xsd:nonNegativeInteger"/>
						<xsd:element name="tp-plus" type="xsd:int"/>
						<xsd:element name="tp-kk-ab" type="xsd:nonNegativeInteger"/>
						<xsd:element name="tp-kk-stufe" type="xsd:nonNegativeInteger"/>
						<xsd:element name="gewicht" type="xsd:nonNegativeInteger"/>
						<xsd:element name="laenge" type="xsd:nonNegativeInteger"/>
						<xsd:element name="bf" type="xsd:int"/>
						<xsd:element name="ini" type="xsd:int"/>
						<xsd:element name="wm-at" type="xsd:int"/>
						<xsd:element name="wm-pa" type="xsd:int"/>
						<xsd:element name="dk" type="xsd:string"/>
						<xsd:element name="wert" type="xsd:string" minOccurs="0"/>
						<xsd:element name="talentId" type="xsd:string" maxOccurs="unbounded"/>
						<xsd:element name="woGetragen" type="xsd:string" minOccurs="0"/>
						<xsd:element name="anmerkung" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
					<xsd:attribute name="bezeichnung" type="xsd:string" use="required"/>
					<xsd:attribute name="einordnung" type="xsd:string" use="optional"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- **********************************  schilde  ********************************* -->
	<xsd:complexType name="schildTyp">
		<xsd:sequence>
			<xsd:element name="schild" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="typ" type="xsd:string"/>
						<xsd:element name="gewicht" type="xsd:nonNegativeInteger"/>
						<xsd:element name="bf" type="xsd:int"/>
						<xsd:element name="ini" type="xsd:int"/>
						<xsd:element name="wm-at" type="xsd:int"/>
						<xsd:element name="wm-pa" type="xsd:int"/>
						<xsd:element name="wert" type="xsd:string" minOccurs="0"/>
						<xsd:element name="woGetragen" type="xsd:string" minOccurs="0"/>
						<xsd:element name="anmerkung" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
					<xsd:attribute name="bezeichnung" type="xsd:string" use="required"/>
					<xsd:attribute name="einordnung" type="xsd:string" use="optional"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ********************************  FK-Waffen  ******************************** -->
	<xsd:complexType name="fkWaffenTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
				tp-W6 - Anzahl an W6
				tp-W20 - Anzahl an W20
				tp-plus - Hinzuaddierte Trefferpunkte
				talentId - Mit welchen Talent(en) diese Waffe angewendet werden kann.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="fkWaffe" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="tp-W6" type="xsd:nonNegativeInteger"/>
						<xsd:element name="tp-W20" type="xsd:nonNegativeInteger"/>
						<xsd:element name="tp-plus" type="xsd:int"/>
						<xsd:element name="tp-plus-reichweite" type="xsd:string"/>
						<xsd:element name="reichweite" type="xsd:string"/>
						<xsd:element name="gewicht" type="xsd:nonNegativeInteger"/>
						<xsd:element name="laenge" type="xsd:nonNegativeInteger"/>
						<xsd:element name="landen" type="xsd:int"/>
						<xsd:element name="ini" type="xsd:int"/>
						<xsd:element name="wert" type="xsd:string" minOccurs="0"/>
						<xsd:element name="talentId" type="xsd:string" maxOccurs="unbounded"/>
						<xsd:element name="woGetragen" type="xsd:string" minOccurs="0"/>
						<xsd:element name="anmerkung" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
					<xsd:attribute name="bezeichnung" type="xsd:string" use="required"/>
					<xsd:attribute name="einordnung" type="xsd:string" use="optional"/>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="geschoss" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="fuerWaffe" type="xsd:string"/>
						<xsd:element name="gewicht" type="xsd:nonNegativeInteger"/>
						<xsd:element name="anzahl" type="xsd:nonNegativeInteger"/>
						<xsd:element name="wert" type="xsd:string" minOccurs="0"/>
						<xsd:element name="woGetragen" type="xsd:string" minOccurs="0"/>
						<xsd:element name="anmerkung" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
					<xsd:attribute name="bezeichnung" type="xsd:string" use="required"/>
					<xsd:attribute name="einordnung" type="xsd:string" use="optional"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ********************************  R�en  ******************************** -->
	<xsd:complexType name="ruestungTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
				tp-W6 - Anzahl an W6
				tp-W20 - Anzahl an W20
				tp-plus - Hinzuaddierte Trefferpunkte
				talentId - Mit welchen Talent(en) diese Waffe angewendet werden kann.
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ruestung" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="zonen" minOccurs="0">
							<xsd:complexType>
								<xsd:attribute name="ko" type="xsd:nonNegativeInteger" use="required"/>
								<xsd:attribute name="br" type="xsd:nonNegativeInteger" use="required"/>
								<xsd:attribute name="rue" type="xsd:nonNegativeInteger" use="required"/>
								<xsd:attribute name="ba" type="xsd:nonNegativeInteger" use="required"/>
								<xsd:attribute name="la" type="xsd:nonNegativeInteger" use="required"/>
								<xsd:attribute name="ra" type="xsd:nonNegativeInteger" use="required"/>
								<xsd:attribute name="lb" type="xsd:nonNegativeInteger" use="required"/>
								<xsd:attribute name="rb" type="xsd:nonNegativeInteger" use="required"/>
								<xsd:attribute name="ges" type="xsd:nonNegativeInteger" use="required"/>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="gRs" type="xsd:nonNegativeInteger" minOccurs="0"/>
						<xsd:element name="gBe" type="xsd:nonNegativeInteger"/>
						<xsd:element name="gewicht" type="xsd:nonNegativeInteger"/>
						<xsd:element name="wert" type="xsd:string"/>
						<xsd:element name="woGetragen" type="xsd:string" minOccurs="0"/>
						<xsd:element name="anmerkung" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
					<xsd:attribute name="bezeichnung" type="xsd:string" use="required"/>
					<xsd:attribute name="einordnung" type="xsd:string" use="optional"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- ********************************  Tiere  ******************************** -->
	<xsd:complexType name="tierTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
				F�rische Begeleiter und "Vertraute".
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="tier" minOccurs="0" maxOccurs="unbounded">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="eigenschaften" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="MU" type="heldElementTyp"/>
									<xsd:element name="KL" type="heldElementTyp"/>
									<xsd:element name="IN" type="heldElementTyp"/>
									<xsd:element name="CH" type="heldElementTyp"/>
									<xsd:element name="FF" type="heldElementTyp"/>
									<xsd:element name="GE" type="heldElementTyp"/>
									<xsd:element name="KO" type="heldElementTyp"/>
									<xsd:element name="KK" type="heldElementTyp"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="erweiterteEigenschaften">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="LeP" type="heldElementTyp"/>
									<xsd:element name="AsP" type="heldElementTyp"/>
									<xsd:element name="AuP" type="heldElementTyp"/>
									<xsd:element name="KA" type="heldElementTyp"/>
									<xsd:element name="MR" type="heldElementTyp"/>
									<xsd:element name="INI" type="heldElementTyp"/>
									<xsd:element name="PA" type="heldElementTyp"/>
									<xsd:element name="natRS" type="heldElementTyp"/>
									<xsd:element name="LO" type="heldElementTyp"/>
									<xsd:element name="FS" type="heldElementTyp"/>
									<xsd:element name="GS" type="heldElementTyp"/>
									<xsd:element name="GW" type="heldElementTyp"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="angriff" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:attribute name="DK" type="xsd:string" use="required"/>
								<xsd:attribute name="TP" type="xsd:string" use="required"/>
								<xsd:attribute name="AT" type="xsd:string" use="required"/>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="vorteile">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="vorteil" type="xsd:string"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="nachteile">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="nachteile" type="xsd:string"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="sonderfertigkeiten">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="sonderf" type="xsd:string"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="ausruestung" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="gegenstand" type="xsd:string"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="zucht" type="xsd:string" minOccurs="0"/>
						<xsd:element name="ausbildung" type="xsd:string"/>
						<xsd:element name="farbe" type="xsd:string"/>
						<xsd:element name="aussehen" type="xsd:string"/>
						<xsd:element name="anmerkung" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
					<xsd:attribute name="tierart" type="xsd:string" use="required"/>
					<xsd:attribute name="namen" type="xsd:string" use="required"/>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<!-- **************************************************************************** -->
	<!-- ******************************  HilfsTypen  ******************************** -->
	<xsd:complexType name="heldElementTyp">
		<xsd:annotation>
			<xsd:documentation xml:lang="DE">
				id - Das eigentliche Ziel, wie bei "Schwerter 5" -> Schwerter
				stufe - Die aktuelle Stufe in dem Wert
				text - Z.B. Vorurteile gegen "Orks"
				linkId - Z.B. Begabt f�hwerter"
				log - Eine Art "LogBuch", d.h. ein Protokol aller îderungen seit
			</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="text" type="xsd:string" minOccurs="0"/>
