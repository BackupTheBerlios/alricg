<n:alricgXML xmlns:n="http://alricg.die3sphaere.de/lib/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://alricg.die3sphaere.de/lib/
..\schema\library-schema.xsd">
	<preamble language="DE" version="v0.1" author="V.Strelow">
		The german standard text-library for Alricg-Project. 
		You can change the text, but DON'T CHANGE THE KEY-WORD!!
		The text is identified by the key-word.
		
		library-element has 3 under-elements:
			short - Text has only one word.
			middle - Text has two or three words.
			long - Text has more as three words.
	</preamble>
	<library>
		<short> <!-- Nur ein Wort -->
			<entry key="MU">MU</entry>
			<entry key="KL">KL</entry>
			<entry key="IN">IN</entry>
			<entry key="CH">CH</entry>
			<entry key="FF">FF</entry>
			<entry key="GE">GE</entry>
			<entry key="KO">KO</entry>
			<entry key="KK">KK</entry>
			<entry key="INI">INI</entry>
			<entry key="MR">MR</entry>
			<entry key="LeP">LeP</entry>
			<entry key="AsP">AsP</entry>
			<entry key="AuP">AuP</entry>
			<entry key="KA">KA</entry>
			<entry key="SO">SO</entry>
			<entry key="Mut">Mut</entry>
			<entry key="Klugheit">Klugheit</entry>
			<entry key="Intuition">Intuition</entry>
			<entry key="Charisma">Charisma</entry>
			<entry key="Fingerfertigkeit">Fingerfertigkeit</entry>
			<entry key="Gewandheit">Gewandheit</entry>
			<entry key="Konstitution">Konstitution</entry>
			<entry key="Körperkraft">Körperkraft</entry>
			<entry key="Initiative">Initiative</entry>
			<entry key="Magieresistens">Magieresistens</entry>
			<entry key="Lebenspunkte">Lebenspunkte</entry>
			<entry key="Astralpunkte">Astralpunkte</entry>
			<entry key="Ausdauerpunkt">Ausdauerpunkt</entry>
			<entry key="Karmaernergie">Karmaernergie</entry>
			<entry key="Sozialstatus">Sozialstatus</entry>
			<entry key="X">X</entry>
    
		</short>
		<middle>
			<entry key="x">Zwei bis 3 Wörter</entry>
		</middle>
		<long>
			<entry key="x">Alles mehr als 3 Wörter</entry>
		</long>
	</library>
</n:alricgXML>
